<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="udb.sv.com.analisisresultados.Vista.Comeplementario4"
    android:background="#aa000000"
    >

    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:columnCount="2"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        >

        <Button
            android:layout_height="150dp"
            android:layout_width="150dp"
            android:text="Login"
            android:gravity="center"
            android:drawableLeft="@drawable/ic_lock_open_black_24dp"
            android:onClick="login"
            />

        <Button
            android:layout_height="150dp"
            android:layout_width="150dp"
            android:text="Llamada"
            android:gravity="center"
            android:drawableLeft="@drawable/ic_call_black_24dp"
            android:onClick="llamada"
            />
        <Button
            android:layout_height="150dp"
            android:layout_width="150dp"
            android:text="Temp"
            android:gravity="center"
            android:drawableLeft="@drawable/ic_wb_cloudy_black_24dp"
            android:onClick="temperatura"
            />
        <Button
            android:layout_height="150dp"
            android:layout_width="150dp"
            android:text="N/A"
            android:gravity="center"
            android:drawableLeft="@drawable/ic_error_black_24dp"
            android:onClick="mostrarMensasje"
            />

    </GridLayout>

</RelativeLayout>
